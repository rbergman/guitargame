  },
  "devDependencies": {
    "@eslint-community/eslint-plugin-eslint-comments": "^4.4.0",
    "@tailwindcss/vite": "^4.0.0",
    "husky": "^9.0.0",
    "pitchfinder": "^2.3.4",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.7.0",
    "typescript-eslint": "^8.0.0",
    "vite": "^6.0.0",
    "vitest": "^2.0.0"
  }
}
